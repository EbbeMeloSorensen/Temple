<UserControl x:Class="Temple.UI.WPF.DD.Wilderness.WildernessView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:Temple.UI.WPF.DD.Wilderness"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" d:DataContext="{d:DesignData WildernessViewModel}">
    <StackPanel>
        <Button Margin="20" Width="100" Content="Go to Mine" Command="{Binding GoToSite_Mine_Command}"/>
        <Button Margin="20" Width="100" Content="Go to Village" Command="{Binding GoToSite_Village_Command}"/>
    </StackPanel>
</UserControl>
